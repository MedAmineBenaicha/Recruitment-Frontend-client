 window.Echo.channel(\"chat1\").listen(\"NewChatMessage\", (e) => {\r\n        console.log(\"lisssste\");\r\n        console.log(e);\r\n        this.getMessages();\r\n      });\r\n    },\r\n    connectMessages() {\r\n      this.intervalid = setInterval(\r\n        function() {\r\n          if (this.client != null) {\r\n            this.getMessages();\r\n          } else {\r\n            return;\r\n          }\r\n        }.bind(this),\r\n        3000\r\n      );\r\n    },\r\n    showPopUp() {\r\n      this.showChatPopUp = !this.showChatPopUp;\r\n    },\r\n    getRooms() {\r\n      this.isLoadingRooms = true;\r\n      this.$store\r\n        .dispatch(\"getRooms\")\r\n        .then(() => {\r\n          this.isLoadingRooms = false;\r\n          //this.getMessages();\r\n        })\r\n        .catch((error) => {\r\n          this.isLoadingRooms = false;\r\n          console.log(error.message);\r\n        });\r\n    },\r\n    getMessages() {\r\n      this.isLoadingMessages = true;\r\n      this.$store\r\n        .dispatch(\"getMessages\", { room_id: 1 })\r\n        .then(() => {\r\n          this.isLoadingMessages = false;\r\n          this.scrollToBottomWhenMessageIsReceived();\r\n        })\r\n        .catch((error) => {\r\n          this.isLoadingMessages = false;\r\n          console.log(error.message);\r\n        });\r\n    },\r\n    sendMessage() {\r\n      if (this.message == null) {\r\n        return;\r\n      }\r\n      const message = new FormData();\r\n      message.append(\"message\", this.message);\r\n      message.append(\"chat_room_id\", this.currentRoom.id);\r\n      message.append(\"client_id\", this.client.id);\r\n      this.$store\r\n        .dispatch(\"sendMessage\", {\r\n          room_id: this.currentRoom.id,\r\n          message: message,\r\n        })\r\n        .then(() => {\r\n          this.message = null;\r\n          this.getMessages();\r\n        })\r\n        .catch(() => {});\r\n    },\r\n    scrollToBottomWhenMessageIsReceived(){\r\n      console.log('xxx');\r\n      $(\".Messages\").stop().animate({ scrollTop: $(\".Messages\")[0].scrollHeight}, 1000);\r\n    }\r\n  },\r\n  created() {\r\n    console.log(\"getRooms\");\r\n    this.getRooms();\r\n  },\r\n  mounted() {\r\n    this.connect();\r\n    //this.connectMessages();\r\n    $(document).ready(function() {\r\n      $(\".chat_on\").click(function() {\r\n        $(\".Layout\").toggle();\r\n        $(\".Messages\").stop().animate({ scrollTop: $(\".Messages\")[0].scrollHeight}, 1000);\r\n        $(\".chat_on\").hide(300);\r\n        $(\".chat_on\").removeClass(\"d-flex\");\r\n      });\r\n\r\n      $(\".chat_close_icon\").click(function() {\r\n        $(\".Layout\").hide();\r\n        $(\".chat_on\").show(300);\r\n        $(\".chat_on\").addClass(\"d-flex\");\r\n      });\r\n    });\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.chat-logo-container,\r\n.chat-container {\r\n  background-color: #f75c6a;\r\n  color: #fff;\r\n  position: fixed;\r\n  z-index: 2147483003;\r\n  bottom: 20px;\r\n  right: 20px;\r\n  width: 60px;\r\n  height: 60px;\r\n  border-radius: 50%;\r\n  background: #fb5f66;\r\n  cursor: pointer;\r\n  box-shadow: 0 1px 6px 0 rgba(0, 0, 0, 0.06), 0 2px 32px 0 rgba(0, 0, 0, 0.16);\r\n  animation: intercom-lightweight-app-launcher 250ms ease;\r\n}\r\n.chat-logo-container i {\r\n  font-size: 2rem;\r\n}\r\n.card {\r\n  border-radius: 15px;\r\n  max-width: 380px;\r\n}\r\n.card-header {\r\n  background-color: #fb5f66;\r\n}\r\n.card-body {\r\n  background-color: #fff;\r\n}\r\n.messenger-header {\r\n  background-color: rgb(43, 168, 72);\r\n  color: rgb(255, 255, 255);\r\n}\r\n/*===========================\r\n     CHAT BOOT MESSENGER\r\n   ===========================*/\r\n#Smallchat .Messages,\r\n#Smallchat .Messages_list {\r\n  -webkit-box-flex: 1;\r\n  -webkit-flex-grow: 1;\r\n  -ms-flex-positive: 1;\r\n  flex-grow: 1;\r\n}\r\n.chat_close_icon {\r\n  cursor: pointer;\r\n  color: #fff;\r\n  font-size: 16px;\r\n  position: absolute;\r\n  righ