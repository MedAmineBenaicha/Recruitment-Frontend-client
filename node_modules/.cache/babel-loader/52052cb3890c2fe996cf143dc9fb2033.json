            <input\r\n              type=\"number\"\r\n              class=\"form-control input-lg\"\r\n              :class=\"validationForEmail\"\r\n              id=\"exampleInputEmail1\"\r\n              aria-describedby=\"emailHelp\"\r\n              placeholder=\"Enter email\"\r\n              v-model=\"codeVerification\"\r\n            />\r\n            <!-- If the errors is empty -->\r\n            <div v-if=\"!validationEmail\" class=\"valid-feedback\">\r\n              Looks good!\r\n            </div>\r\n            <!-- If the errors is full -->\r\n            <div v-if=\"validationEmail\" class=\"invalid-feedback\">\r\n              <span\r\n                class=\"d-block\"\r\n                v-for=\"(error, index) in emailErrors\"\r\n                :key=\"index\"\r\n              >\r\n                {{ error }}\r\n              </span>\r\n            </div>\r\n          </div>\r\n\r\n          <!-- New Password -->\r\n          <div class=\"form-group mb-4\">\r\n            <label for=\"exampleInputEmail1\">\r\n              <i class=\"fas fa-envelope mr-2\"></i> New Password :\r\n            </label>\r\n            <input\r\n              type=\"password\"\r\n              class=\"form-control input-lg\"\r\n              :class=\"validationForEmail\"\r\n              id=\"exampleInputEmail1\"\r\n              aria-describedby=\"emailHelp\"\r\n              v-model=\"password\"\r\n            />\r\n            <!-- If the errors is empty -->\r\n            <div v-if=\"!validationEmail\" class=\"valid-feedback\">\r\n              Looks good!\r\n            </div>\r\n            <!-- If the errors is full -->\r\n            <div v-if=\"validationEmail\" class=\"invalid-feedback\">\r\n              <span\r\n                class=\"d-block\"\r\n                v-for=\"(error, index) in emailErrors\"\r\n                :key=\"index\"\r\n              >\r\n                {{ error }}\r\n              </span>\r\n            </div>\r\n          </div>\r\n\r\n          <!-- Confirm Password -->\r\n          <div class=\"form-group mb-4\">\r\n            <label for=\"exampleInputEmail1\">\r\n              <i class=\"fas fa-envelope mr-2\"></i> Confirm Password :\r\n            </label>\r\n            <input\r\n              type=\"password\"\r\n              class=\"form-control input-lg\"\r\n              :class=\"validationForEmail\"\r\n              id=\"exampleInputEmail1\"\r\n              aria-describedby=\"emailHelp\"\r\n              v-model=\"confirmPassword\"\r\n            />\r\n            <!-- If the errors is empty -->\r\n            <div v-if=\"!validationEmail\" class=\"valid-feedback\">\r\n              Looks good!\r\n            </div>\r\n            <!-- If the errors is full -->\r\n            <div v-if=\"validationEmail\" class=\"invalid-feedback\">\r\n              <span\r\n                class=\"d-block\"\r\n                v-for=\"(error, index) in emailErrors\"\r\n                :key=\"index\"\r\n              >\r\n                {{ error }}\r\n              </span>\r\n            </div>\r\n          </div>\r\n          <!-- button submit -->\r\n          <div class=\"d-flex align-items-center mt-4\">\r\n            <button\r\n              @click.prevent=\"resetPassword\"\r\n              class=\"btn btn-primary btn-submit mx-auto\"\r\n            >\r\n              <i class=\"fas fa-sign-in-alt mr-2\"></i> Reset Password\r\n            </button>\r\n          </div>\r\n        </form>\r\n      </div>\r\n    </div>\r\n  </section>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      email: null,\r\n      password: null,\r\n      codeVerification: null,\r\n      confirmPassword: null,\r\n    };\r\n  },\r\n  methods: {\r\n    resetPassword() {\r\n      if (\r\n        this.email == null ||\r\n        this.codeVerification == null ||\r\n        this.password == null\r\n      ) {\r\n        return;\r\n      }\r\n\r\n      const resetData = new FormData();\r\n      resetData.append(\"email\", this.email);\r\n      resetData.append(\"verification_code\", parseInt(this.codeVerification));\r\n      resetData.append(\"password\", this.password);\r\n\r\n      this.$store.dispatch(\"resetPassword\", resetData);\r\n    },\r\n  },\r\n  mounted() {\r\n    this.email = this.$route.params.email;\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\nsection {\r\n  background-color: #f0f4f8;\r\n}\r\n.forgot-password-container {\r\n  max-width: 540px;\r\n  background-color: #fff;\r\n  border-radius: 10px;\r\n}\r\n.formConnexion label {\r\n  color: #788291;\r\n  font-size: 16px;\r\n  margin-left: 10px;\r\n  font-family: \"GiltroyRegular\" sans-serif;\r\n}\r\n.formConnexion-link {\r\n  color: #06adef;\r\n  font-size: 16px;\r\n  font-family: \"GiltroyRegular\" sans-serif;\r\n  font-weight: 400;\r\n  line-height: 1;\r\n  letter-spacing: 0.01;\r\n}\r\n.formConnexion-link:hover {\r\n  color: #06adef;\r\n}\r\n\r\n.btn-submit {\r\n  border-color: #3dad45;\r\n  padding: 10px 35px;\r\n  background-color: #50c058 !important;\r\n  color: #fff;\r\n  font-weight: 550;\r\n}\r\n.btn-submit:hover {\r\n  border-color: #3dad45;\r\n}\r\n</style>\r\n"],"sourceRoot":""}]}