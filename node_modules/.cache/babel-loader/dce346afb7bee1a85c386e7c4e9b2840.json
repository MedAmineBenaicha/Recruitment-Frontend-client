\nimport \"bootstrap/dist/js/bootstrap.js\";\r\n// Use v-calendar & v-date-picker components\r\n\r\ncreateApp(App)\r\n  .use(VueFinalModal())\r\n  .use(VCalendar, {})\r\n  .use(store)\r\n  .use(router)\r\n  .mount(\"#app\");\r\n"]}]}