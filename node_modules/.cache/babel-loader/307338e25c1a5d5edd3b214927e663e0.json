n            </div>\r\n            <div class=\"mission-details d-flex justify-content-between\">\r\n              <h4 class=\"text-left\">\r\n                End Date :\r\n              </h4>\r\n              <p>{{ mission.end_date }}</p>\r\n            </div>\r\n            <div class=\"mission-details d-flex justify-content-between\">\r\n              <h4 class=\"text-left\">\r\n                Duration :\r\n              </h4>\r\n              <p>\r\n                {{\r\n                  differenceBetweenTwoDates(\r\n                    mission.start_date,\r\n                    mission.end_date\r\n                  )\r\n                }}\r\n              </p>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </section>\r\n</template>\r\n\r\n<script>\r\nimport ClipLoader from \"vue-spinner/src/ClipLoader.vue\";\r\nexport default {\r\n  components: {\r\n    ClipLoader,\r\n  },\r\n  data() {\r\n    return {\r\n        isLoading: false,\r\n    };\r\n  },\r\n  computed: {\r\n    mission() {\r\n      return this.$store.getters.getClientMission;\r\n    },\r\n  },\r\n  methods: {\r\n    differenceBetweenTwoDates(start_date, end_date) {\r\n      var diff = Math.floor(\r\n        (Date.parse(end_date) - Date.parse(start_date)) / 86400000\r\n      );\r\n      return diff;\r\n    },\r\n    getMissionById() {\r\n      this.isLoading = true;\r\n      const client_id = this.$route.params.client_id;\r\n      const mission_id = this.$route.params.mission_id;\r\n      let payload = {\r\n        client_id: client_id,\r\n        mission_id: mission_id,\r\n      };\r\n      this.$store\r\n        .dispatch(\"getMissionById\", payload)\r\n        .then(() => {\r\n            console.log('here');\r\n          this.isLoading = false;\r\n        })\r\n        .catch((error) => {\r\n          this.isLoading = false;\r\n          console.log(error.message);\r\n        });\r\n    },\r\n  },\r\n  created() {\r\n    this.getMissionById();\r\n  },\r\n};\r\n</script>\r\n"],"sourceRoot":""}]}